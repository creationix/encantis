func () {
  // Nested brackets (pointer indirection)
  let a:u8[#][#]      = [ "hello", "world" ]  // slice of slices
  let b:u8[/0][/0]    = [ "hello", "world" ]  // null-term of null-term
  let c:u8[/0][#]     = [ "hello", "world" ]  // null-term of slices
  let d:u8[#][/0]     = [ "hello", "world" ]  // slice of null-term

  // Merged brackets (flat memory)
  let e:u8[/u8/u8]    = [ "hello", "world" ]  // prefix count + prefix len per elem
  let f:u8[/0/0]      = [ "hello", "world" ]  // double null-terminated
  let g:u8[/leb128/0] = [ "hello", "world" ]  // leb128 count + null-term per elem
}
