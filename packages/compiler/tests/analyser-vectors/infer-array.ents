func () {
  // Nested brackets (pointer indirection)
  let a:[][]u8         = [ "hello", "world" ]  // slice of slices
  let b:[*:0][*:0]u8   = [ "hello", "world" ]  // null-term of null-term
  let c:[*:0][]u8      = [ "hello", "world" ]  // null-term of slices
  let d:[][*:0]u8      = [ "hello", "world" ]  // slice of null-term

  // Merged brackets (flat memory)
  let e:[*:?:?]u8      = [ "hello", "world" ]  // prefix count + prefix len per elem
  let f:[*:0:0]u8      = [ "hello", "world" ]  // double null-terminated
  let g:[*:?:0]u8      = [ "hello", "world" ]  // leb128 count + null-term per elem
}
