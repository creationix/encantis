{
  "type": "dark",
  "colors": {
    "editor.background": "#1f1d1b",
    "editor.foreground": "#fefcee",
    "editorIndentGuide.background": "#404040",
    "editorRuler.foreground": "#333333",
    "activityBarBadge.background": "#007acc",
    "sideBarTitle.foreground": "#bbbbbb"
  },
  "tokenColors": [
    {
      "name": "dark-blue - symbols",
      "scope": "comment",
      "settings": {
        "foreground": "#6967f5",
        // "fontStyle": "italic"
      }
    },
    {
      "name": "light-blue - library variables",
      "scope": "comment",
      "settings": {
        "foreground": "#16b05b",
        // "fontStyle": "italic"
      }
    },
    {
      "name": "dark-green - user variables",
      "scope": "comment",
      "settings": {
        "foreground": "#029dba",
        // "fontStyle": "italic"
      }
    },
    {
      "name": "light-green - builtin/tag/keyword",
      "scope": "comment",
      "settings": {
        "foreground": "#029dba",
        // "fontStyle": "italic"
      }
    },
    /* light-green */
.cm-s-notebook-dark .cm-builtin,
.cm-s-notebook-dark .cm-tag,
.cm-s-notebook-dark .cm-keyword {color:#6ac21d
    }
    /* red */
.cm-s-notebook-dark .cm-operator {color:#f61941
    }
    /* orange */
.cm-s-notebook-dark .cm-string {color:#e05909
    }
.cm-s-notebook-dark .cm-string-2 {color:#F0A713
    }
    /* pink */
.cm-s-notebook-dark .cm-number {color:#ea1b8d
    }
    /* purple */
.cm-s-notebook-dark .cm-atom {color:#9625cf
    }
    /* brown */
.cm-s-notebook-dark .cm-comment {color:#97694C
    }
  ]
}